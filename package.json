{
   "name": "nestjs-typeorm-template",
   "version": "0.0.1",
   "description": "Nest + typeorm-typescript + JWT + Swagger",
   "keywords": [
      "nest",
      "nestjs",
      "nest.js",
      "typeorm",
      "typeorm-typescript",
      "orm",
      "nodejs",
      "node.js",
      "node",
      "typescript",
      "jwt",
      "jsonwebtoken",
      "passport",
      "swagger"
   ],
   "author": "vseriousv",
   "private": true,
   "license": "UNLICENSED",
   "scripts": {
      "prebuild": "rimraf dist",
      "build": "nest build",
      "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
      "start": "ts-node src/index.ts",
      "start:dev": "nest start --watch",
      "start:debug": "nest start --debug --watch",
      "start:prod": "node dist/main",
      "typeorm": "yarn run build && npx typeorm -d dist/db/data-source.js",
      "migration:generate": "yarn typeorm -- migration:generate",
      "migration:run": "yarn typeorm -- migration:run",
      "migration:revert": "yarn typeorm -- migration:revert",
      "worker:one": "ts-node -- src/services/workers/worker-one/worker-one.microservice.ts"
   },
   "dependencies": {
      "@nestjs/common": "^8.0.0",
      "@nestjs/config": "^2.3.1",
      "@nestjs/core": "^8.0.0",
      "@nestjs/jwt": "^10.0.2",
      "@nestjs/mapped-types": "*",
      "@nestjs/microservices": "^9.3.9",
      "@nestjs/passport": "^9.0.3",
      "@nestjs/platform-express": "^8.0.0",
      "@nestjs/schedule": "^2.2.0",
      "@nestjs/swagger": "^6.2.1",
      "@nestjs/typeorm": "^9.0.1",
      "bcrypt": "^5.1.0",
      "class-transformer": "^0.5.1",
      "class-validator": "^0.14.0",
      "jsonwebtoken": "^9.0.0",
      "passport": "^0.6.0",
      "passport-jwt": "^4.0.1",
      "path": "^0.12.7",
      "pg": "^8.4.0",
      "prom-client": "^14.2.0",
      "reflect-metadata": "^0.1.13",
      "rimraf": "^3.0.2",
      "rxjs": "^7.2.0",
      "swagger-stats": "^0.99.5",
      "typeorm": "0.3.12"
   },
   "devDependencies": {
      "@nestjs/cli": "^8.0.0",
      "@nestjs/schematics": "^8.0.0",
      "@nestjs/testing": "^8.0.0",
      "@types/express": "^4.17.13",
      "@types/jest": "27.0.2",
      "@types/node": "^16.11.10",
      "@types/supertest": "^2.0.11",
      "@typescript-eslint/eslint-plugin": "^5.0.0",
      "@typescript-eslint/parser": "^5.0.0",
      "dotenv": "^16.0.3",
      "eslint": "^8.0.1",
      "eslint-config-prettier": "^8.3.0",
      "eslint-plugin-prettier": "^4.0.0",
      "jest": "^27.2.5",
      "prettier": "^2.3.2",
      "source-map-support": "^0.5.20",
      "supertest": "^6.1.3",
      "ts-jest": "^27.0.3",
      "ts-loader": "^9.2.3",
      "ts-node": "10.7.0",
      "tsconfig-paths": "^3.10.1",
      "typescript": "4.5.2"
   },
   "jest": {
      "moduleFileExtensions": [
         "js",
         "json",
         "ts"
      ],
      "rootDir": "src",
      "testRegex": ".*\\.spec\\.ts$",
      "transform": {
         "^.+\\.(t|j)s$": "ts-jest"
      },
      "collectCoverageFrom": [
         "**/*.(t|j)s"
      ],
      "coverageDirectory": "../coverage",
      "testEnvironment": "node"
   }
}
